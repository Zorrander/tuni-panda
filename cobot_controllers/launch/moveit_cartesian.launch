<?xml version="1.0" ?>

<launch>
  <arg name="robot_ip" default="130.230.37.115"/>
  <arg name="load_gripper" default="true" />
  <include file="$(find panda_moveit_config)/launch/panda_control_moveit_rviz.launch" >
    <arg name="robot_ip" value="$(arg robot_ip)" />
    <arg name="load_gripper" value="$(arg load_gripper)" />
  </include>


    <node name="cobot_control" type="moveit_cartesian.py" pkg="cobot_controllers" output="screen" />


</launch>
