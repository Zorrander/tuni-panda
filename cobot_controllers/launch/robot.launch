<?xml version="1.0" ?>

<launch>

  <arg name="robot_ip" default="130.230.37.115"/>
  <arg name="load_gripper" default="true" />
  <include file="$(find panda_moveit_config)/launch/panda_control_moveit_rviz.launch" >
    <arg name="robot_ip" value="$(arg robot_ip)" />
    <arg name="load_gripper" value="$(arg load_gripper)" />
  </include>

  <!--<rosparam command="load" file="$(find cobot_controllers)/config/pose-configuration.yaml" /> -->

  <node name="panda_gripper_control" type="gripper.py" pkg="cobot_controllers" output="screen" />

  <node name="panda_arm_control" type="moveit.py" pkg="cobot_controllers" output="screen" />

</launch>
